<launch>
  <node name="ariac_sim" pkg="nist_gear" type="gear.py"
        args="-f
              $(find nist_gear)/config/sample_user_config.yaml
              $(find nist_gear)/config/sample.yaml
              --development-mode
              --verbose --no-gui"/>
  <test pkg="test_ariac" type="ros_api_checker" test-name="ariac_rosapi_development_mode"
        args="$(find test_ariac)/test/ariac_rosapi_development_mode.yaml"
        time-limit="60.0"/>
</launch>
