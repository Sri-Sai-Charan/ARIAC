<launch>
  <node name="ariac_sim" pkg="nist_gear" type="gear.py"
        args="-f
              $(find nist_gear)/config/sample.yaml
              $(find test_ariac)/test/product_drop_over_box.yaml
              $(find test_ariac)/test/tf_sensors.yaml
